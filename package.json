{
  "name": "medieval-dynasty-app",
  "version": "1.0.0",
  "description": "Production planner for Medieval Dynasty",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "prebuild": "run-s clean",
    "build": "parcel build src/index.html --no-source-maps --public-url ./",
    "clean": "rimraf ./dist",
    "start:app": "parcel src/index.html --open",
    "start": "run-p start:*"
  },
  "keywords": [
    "planner",
    "medieval-dynasty",
    "production"
  ],
  "author": "dohrm",
  "license": "MIT",
  "dependencies": {
    "@artsy/fresnel": "^3.2.1",
    "@reduxjs/toolkit": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-table": "^7.7.0",
    "redux": "^4.1.2",
    "semantic-ui-react": "^2.0.4"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.1",
    "@parcel/transformer-typescript-tsc": "^2.0.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-table": "^7.7.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}

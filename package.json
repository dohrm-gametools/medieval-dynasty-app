{
  "name": "medieval-dynasty-app",
  "version": "1.0.0",
  "description": "Production planner for Medieval Dynasty",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "prebuild": "run-s clean lint",
    "build": "parcel build src/index.html --no-source-maps",
    "clean": "rimraf ./dist",
    "lint": "run-s lint:code",
    "lint:code": "eslint --ext ts,tsx src",
    "lint:fix": "eslint --ext ts,tsx src --fix",
    "start:app": "parcel src/index.html --open",
    "start": "run-p start:*"
  },
  "keywords": [
    "planner",
    "medieval-dynasty",
    "production"
  ],
  "author": "dohrm",
  "license": "MIT",
  "dependencies": {
    "@artsy/fresnel": "^3.2.1",
    "@reduxjs/toolkit": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-table": "^7.7.0",
    "redux": "^4.1.2",
    "semantic-ui-react": "^2.0.4"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.1",
    "@parcel/transformer-typescript-tsc": "^2.0.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-table": "^7.7.9",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "staticFiles": {
    "staticPath": [
      "public"
    ],
    "watcherGlob": "**"
  },
  "externals": {
    "config.js?version={{BUILD_TIME}}": false
  }
}

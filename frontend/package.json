{
  "name": "medieval-dynasty-app",
  "version": "1.0.0",
  "description": "Production planner for Medieval Dynasty",
  "scripts": {
    "prebuild": "run-s clean",
    "build": "parcel build src/index.html --public-url /",
    "postbuild": "cp dist/index.html dist/404.html && cp public/CNAME dist/CNAME",
    "clean": "rimraf ./dist",
    "start:app": "parcel watch src/index.html",
    "start": "run-p start:*"
  },
  "keywords": [
    "planner",
    "medieval-dynasty",
    "production"
  ],
  "author": "dohrm",
  "license": "MIT",
  "dependencies": {
    "@artsy/fresnel": "^3.2.1",
    "@auth0/auth0-react": "^1.8.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.8",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/bootstrap-table": "^1.12.0",
    "@types/pouchdb": "^6.4.0",
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "semantic-ui-react": "^2.0.4"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.1",
    "@parcel/transformer-sass": "^2.1.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.1.1",
    "postcss": "^8.4.5",
    "postcss-custom-properties": "^12.0.4",
    "postcss-import": "^14.0.2",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "parcelIgnore": [
    "config.js"
  ]
}
